<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="omniknow.soporte.reportemantenimiento">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="omniknow.soporte.reportemantenimiento.EditarReporte"
            android:label="@string/editar_reporte"/>
        <activity
            android:name="omniknow.soporte.reportemantenimiento.VerReporte"
            android:label="@string/detalles_de_reporte" />
        <activity
            android:name="omniknow.soporte.reportemantenimiento.NuevoReporte"
            android:label="@string/nuevo_reporte" />
        <activity
            android:name="omniknow.soporte.reportemantenimiento.ListaReportes"
            android:label="@string/reportes_de_mantenimiento" />
        <activity android:name="omniknow.soporte.reportemantenimiento.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>